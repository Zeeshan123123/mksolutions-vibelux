{
  "name": "vibelux-app",
  "version": "0.2.12",
  "private": true,
  "engines": {
    "node": "20.x",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:monitor": "node build-monitor.js",
    "vercel-build": "echo 'Starting build...' && prisma generate && NODE_OPTIONS='--max-old-space-size=6144' NEXT_TELEMETRY_DISABLED=1 DISABLE_ESLINT_PLUGIN=true next build",
    "build:vercel": "next build",
    "build:production": "NODE_ENV=production next build",
    "build:aws": "NODE_OPTIONS='--max-old-space-size=131072' NEXT_CONFIG_FILE=next.config.aws.js next build",
    "deploy:aws": "./scripts/deploy-aws.sh",
    "setup:aws": "./scripts/aws-setup.sh",
    "start": "next start",
    "optimize": "./optimize-build.sh",
    "clean": "rm -rf .next node_modules/.cache out dist",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:create": "prisma migrate dev --create-only",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:setup": "./scripts/setup-production-db.sh",
    "stripe:setup": "tsx src/scripts/setup-stripe-products.ts",
    "stripe:add-to-vercel": "bash scripts/add-stripe-to-vercel.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:integration:watch": "jest --config jest.integration.config.js --watch",
    "test:integration:coverage": "jest --config jest.integration.config.js --coverage",
    "test:all": "npm run test && npm run test:integration",
    "pre-deploy": "node scripts/pre-deploy-check.js",
    "deploy:check": "npm run pre-deploy && npm run build",
    "format:check": "prettier --check .",
    "test:api": "jest --testPathPattern=__tests__/api",
    "test:lib": "jest --testPathPattern=__tests__/lib",
    "test:unit": "jest --testPathPattern=src/__tests__/unit",
    "test:cad": "jest --testPathPattern=src/tests",
    "test:e2e": "playwright test",
    "test:load": "k6 run tests/load/load-test.js",
    "test:smoke": "playwright test --grep @smoke",
    "test:ai": "node test-ai-setup.js",
    "check-errors": "node scripts/check-errors.js",
    "check-errors:v2": "node scripts/check-errors-v2.js",
    "check-polyfills": "node scripts/polyfill-check.js",
    "check-all": "npm run check-errors:v2 && npm run check-polyfills",
    "worker": "tsx src/workers/index.ts",
    "worker:dev": "tsx watch src/workers/index.ts",
    "worker:prod": "node dist/workers/index.js",
    "db:indexes": "prisma migrate deploy && psql $DATABASE_URL -f prisma/migrations/20240124_performance_indexes/migration.sql",
    "redis:start": "redis-server",
    "influx:start": "docker run -d -p 8086:8086 influxdb:2.7",
    "db:reset": "prisma migrate reset --force",
    "db:seed-demo": "npx tsx src/scripts/seed-demo-data.ts",
    "db:reset-demo": "npx prisma db push --force-reset && npm run db:seed && npm run db:seed-demo",
    "backup:create": "tsx src/scripts/create-backup.ts",
    "backup:restore": "tsx src/scripts/restore-backup.ts",
    "security:audit": "npm audit && npx better-npm-audit audit",
    "security:check": "npx snyk test",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:bundle": "npx @next/bundle-analyzer",
    "lighthouse": "lhci autorun",
    "validate:env": "tsx scripts/validate-environment.ts",
    "start:cron": "tsx -e \"import { getCronRunner } from './src/services/cron-runner'; getCronRunner().start()\"",
    "stop:cron": "tsx -e \"import { getCronRunner } from './src/services/cron-runner'; getCronRunner().stop()\"",
    "start:energy-monitoring": "tsx -e \"import { EnergyMonitoringService } from './src/services/energy-monitoring-service'; new EnergyMonitoringService().start()\"",
    "websocket:dev": "nodemon scripts/start_websocket.js",
    "websocket:start": "node scripts/start_websocket.js",
    "automation:dev": "cd scripts && python3 greenhouse_automation.py",
    "automation:setup": "cd scripts && ./setup_automation.sh",
    "test:influxdb": "tsx test-influxdb.ts",
    "setup:revenue-sharing": "npm run validate:env && npm run db:generate && echo 'ðŸŽ‰ Revenue sharing setup complete!'",
    "deploy:update": "tsx scripts/deploy-update.ts",
    "increase:rollout": "tsx scripts/deploy-update.ts increase-rollout",
    "emergency:disable": "tsx scripts/deploy-update.ts emergency-disable",
    "migrate:production": "npx prisma migrate deploy",
    "backup:production": "tsx scripts/backup-production.ts",
    "test:hlp": "tsx scripts/test-hlp.ts",
    "stripe:webhooks": "tsx src/scripts/setup-stripe-webhooks.ts",
    "stripe:listen": "stripe listen --forward-to localhost:3002/api/stripe/webhook",
    "build:fast": "NODE_OPTIONS='--max-old-space-size=16384' NODE_ENV=production NEXT_TELEMETRY_DISABLED=1 DISABLE_ESLINT_PLUGIN=true next build",
    "build:analyze": "cross-env ANALYZE=true npm run build:fast",
    "upload-sendgrid-templates": "node scripts/upload-sendgrid-templates.js"
  },
  "turbo": {
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          ".next/**",
          "dist/**",
          "build/**"
        ],
        "inputs": [
          "**/*",
          "!.next/**",
          "!node_modules/**"
        ]
      },
      "lint": {
        "outputs": []
      },
      "test": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "coverage/**"
        ]
      },
      "dev": {
        "cache": false
      }
    }
  },
